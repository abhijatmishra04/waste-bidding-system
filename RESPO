        try {
            payment.setNxtTranDt(DateUtils.parseDate(rs.getString("NXT_TRAN_DT")));
        } catch (Exception e) {
            log.error("Error parsing NXT_TRAN_DT", e);
        }

        try {
            payment.setLastTranDt(DateUtils.parseDate(rs.getString("LST_TRAN_DT")));
        } catch (Exception e) {
            log.error("Error parsing LST_TRAN_DT", e);
        }

        try {
            payment.setClosedDate(DateUtils.parseDate(rs.getString("CLS_DT")));
        } catch (Exception e) {
            log.error("Error parsing CLS_DT", e);
        }

        try {
            payment.setFrstTranDt(DateUtils.parseDate(rs.getString("FRST_TRAN_DT")));
        } catch (Exception e) {
            log.error("Error parsing FRST_TRAN_DT", e);
        }

        try {
            payment.setDueDate(DateUtils.parseDate(rs.getString("DUE_DT")));
        } catch (Exception e) {
            log.error("Error parsing DUE_DT", e);
        }
