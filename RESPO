@Bean
public Step processCustagFileStep() {
    return stepBuilderFactory.get("processCustagFileStep")
            .<CustomRecordDTO, EnrichedRecordDTO>chunk(10)
            .reader(custagCompFileReader) // Reader for the 2nd file
            .processor(custagProcessor)  // Processor for matching and enriching
            .writer(temporaryWriter)     // Temporary Writer
            .build();
}
