SELECT 
    schd.schd_id, 
    schd.schd_stat, 
    schd.xfer_ref_id, 
    acc.account_boa_id 
FROM 
    account_boa acc 
JOIN 
    schedule_m2m schd 
ON 
    acc.account_boa_id = schd.schd_id 
WHERE 
    acc.acc_no = ? 
    AND acc.ent_cd = ? 
    AND acc.prod_cd = ? 
    AND schd.schd_stat = 'S01' 
    AND schd.xfer_ref_id IS NULL;
