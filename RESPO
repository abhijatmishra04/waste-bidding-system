

    public static final String CHUNK_SIZE = "sda.response.batch.chunk.size";
    public static final String PARTITION_GRID_SIZE = "sda.response.batch.partition.grid.size";
    public static final String THREAD_CORE_POOL_SIZE = "sda.response.batch.thread.core.pool.size";
    public static final String THREAD_MAX_POOL_SIZE = "sda.response.batch.thread.max.pool.size";
    public static final String BATCH_QUEUE_CAPACITY = "sda.response.batch.queue.capacity";
    public static final String THREAD_PREFIX_NAME = "sda.response.batch.thread.prefix.name";
    public static final String PARTITION_TYPE = "sda.response.batch.partition.type";

Integer.parseInt(dynamicConfigPropRepository.findPropertyValueByPropertyName(BatchConstants.CHUNK_SIZE));
Integer.parseInt(dynamicConfigPropRepository.findPropertyValueByPropertyName(BatchConstants.PARTITION_GRID_SIZE));
Integer.parseInt(dynamicConfigPropRepository.findPropertyValueByPropertyName(BatchConstants.THREAD_CORE_POOL_SIZE));
Integer.parseInt(dynamicConfigPropRepository.findPropertyValueByPropertyName(BatchConstants.THREAD_MAX_POOL_SIZE));
Integer.parseInt(dynamicConfigPropRepository.findPropertyValueByPropertyName(BatchConstants.BATCH_QUEUE_CAPACITY));
dynamicConfigPropRepository.findPropertyValueByPropertyName(BatchConstants.THREAD_PREFIX_NAME);
dynamicConfigPropRepository.findPropertyValueByPropertyName(BatchConstants.PARTITION_TYPE);
